namespace sola.lang

use sola.lang.RuntimeException;

/**
 * 空引用异常
 * 当尝试访问空引用的成员时抛出
 */
public class NullReferenceException extends RuntimeException {
    public function __construct(string $message = "Object reference not set to an instance of an object", int $code = 0, ?Throwable $previous = null) {
        parent::__construct($message, $code, $previous);
    }
}







