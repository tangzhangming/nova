namespace sola.lang

use sola.lang.ArithmeticException;

/**
 * 除零异常
 * 当尝试除以零时抛出
 */
public class DivideByZeroException extends ArithmeticException {
    public function __construct(string $message = "Attempted to divide by zero", int $code = 0, ?Throwable $previous = null) {
        parent::__construct($message, $code, $previous);
    }
}





